#!/usr/bin/env bash

BIN_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"       # Get directory of this file
PROJECT_DIR=$(dirname $BIN_DIR)                                   # Get root directory of project
source $BIN_DIR/common/setup $1

cd $VARIANT_DIR && rm -rf $BUILD_DIR_NAME

echo "----------------------"

echo 
echo "cd $PROJECT_DIR"
cd $PROJECT_DIR

echo 
echo "pwd"
pwd

echo 
echo "ls -l"
ls -l

echo 
echo "mkdir -p build/release/package/"
mkdir -p build/release/package/

# echo 
# echo "cp notes.txt build/release/package/robo3t-1.3.0-linux-x86_64-3d37f2e.tar.gz"
# cp notes.txt build/release/package/robo3t-1.3.0-linux-x86_64-3d37f2e.tar.gz

echo 
echo "cp notes.txt build/release/package/"
cp notes.txt build/release/package/

echo 
echo "ls -l build/release/package/"
ls -l build/release/package/
